    "sub = x - y\n",
    "mul = x * y\n",
    "avg = sum / 2\n",
    "num_max = max (x, y)\n",
    "num_min = min (x, y)\n",
    "\n",
    "print(\"합 %d\" %sum)\n",
    "print(\"차 %d\" %sub)\n",
    "print(\"곱 %d\" %mul)\n",
    "print(\"평균 %d\" %avg)\n",
    "print(\"큰 수 %d\" %num_max)\n",
    "print(\"작은 수 %d\" %num_min)\n",
    "\n"
